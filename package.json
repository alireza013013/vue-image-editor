{
  "name": "vue3-image-editor",
  "version": "1.0.3",
  "description": "A Image Editor component library",
  "main": "./dist/vue-image-editor.umd.js",
  "module": "./dist/vue-image-editor.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-image-editor.esm.js",
      "require": "./dist/vue-image-editor.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "library",
    "image",
    "edit",
    "typescript"
  ],
  "dependencies": {
    "compressorjs": "^1.2.1",
    "path": "^0.12.7",
    "pinia": "^2.1.4",
    "vite-plugin-dts": "^3.5.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vue-tsc": "^1.8.6"
  }
}
